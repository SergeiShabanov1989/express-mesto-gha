{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": [
      "airbnb-base/legacy"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "eslint linebreak-style": 0,
    "no-underscore-dangle": "off",
    "eol-last": 0,
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    "quotes": "off"
  }
}